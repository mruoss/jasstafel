{
	"name": "jasstafel",
	"version": "1.0.0",
	"description": "Jasstafel zum Schreiben von Jasspartien",
	"license": "MIT",
	"scripts": {
		"build": "webpack --mode production --env.WEBPACK_MODE=production",
		"lint": "yarn run lint:js && yarn run lint:sass",
		"lint:js": "eslint src/js/",
		"lint:sass": "sass-lint -c .sass-lint.yml -v -q --max-warnings 0",
		"prettier": "prettier --write '**/*.js'",
		"start": "webpack-dev-server --content-base public/ --open --mode development --env.WEBPACK_MODE=development",
		"test": "mocha --require babel-register ./tests/**/*.js"
	},
	"dependencies": {
		"fp": "^0.1.0",
		"konva": "^2.1.6",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-konva": "^1.7.7",
		"react-redux": "^5.0.7",
		"react-sizes": "^1.0.4",
		"recompose": "^0.27.1",
		"redux": "^4.0.0",
		"redux-undo": "^1.0.0-beta9-9-7",
		"reselect": "^3.0.1"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.1.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.5",
		"babel-loader": "^7.1.4",
		"babel-plugin-module-resolver": "^3.1.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"chai": "^4.1.2",
		"chai-match-pattern": "^1.1.0",
		"css-loader": "^0.28.11",
		"eslint": "^5.0.1",
		"eslint-config-prettier": "^2.9.0",
		"eslint-import-resolver-babel-module": "^4.0.0",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-css-modules": "^2.7.5",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jasmine": "^2.10.1",
		"eslint-plugin-lodash-fp": "^2.1.3",
		"eslint-plugin-prettier": "^2.6.1",
		"eslint-plugin-react": "^7.10.0",
		"file-loader": "^1.1.11",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.0",
		"mocha": "^5.2.0",
		"node-sass": "^4.9.0",
		"postcss-loader": "^2.1.5",
		"prettier": "^1.13.6",
		"prop-types": "^15.6.2",
		"sass-lint": "^1.12.1",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.21.0",
		"url-loader": "^1.0.1",
		"webpack": "^4.12.2",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	}
}
