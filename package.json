{
	"name": "jasstafel",
	"version": "2.2.1",
	"description": "Jasstafel zum Schreiben von Jasspartien",
	"license": "MIT",
	"scripts": {
		"build": "webpack --mode production --env presets=serviceworker",
		"lint": "yarn run lint:js && yarn run lint:sass",
		"lint:js": "eslint src/js/",
		"lint:sass": "sass-lint -c .sass-lint.yml -v -q --max-warnings 0",
		"prettier": "prettier --write '**/*.js'",
		"start": "webpack serve --static-public-path public/ --open --mode development --hot",
		"start:sw": "webpack serve --static-public-path public/ --open --mode development --env presets=serviceworker",
		"test": "mocha --require babel-register ./tests/**/*.js"
	},
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "5.15.4",
		"bluebird": "3.7.2",
		"core-js": "3.18.2",
		"konva": "8.2.2",
		"lodash": "4.17.21",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-konva": "17.0.2-5",
		"react-redux": "7.2.5",
		"redux": "4.1.1",
		"redux-undo": "1.0.1",
		"reselect": "4.0.0",
		"styled-components": "5.3.1",
		"workbox-precaching": "6.3.0",
		"workbox-webpack-plugin": "6.3.0"
	},
	"devDependencies": {
		"@babel/core": "7.15.8",
		"@babel/plugin-proposal-object-rest-spread": "7.15.6",
		"@babel/preset-env": "7.15.8",
		"@babel/preset-react": "7.14.5",
		"ansi-html": "npm:ansi-html-community@0.0.8",
		"babel-eslint": "10.1.0",
		"babel-loader": "8.2.2",
		"babel-plugin-module-resolver": "4.1.0",
		"babel-polyfill": "6.26.0",
		"chai": "4.3.4",
		"chai-match-pattern": "1.2.0",
		"clean-webpack-plugin": "4.0.0",
		"copy-webpack-plugin": "9.0.1",
		"css-loader": "6.3.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-babel-module": "5.3.1",
		"eslint-plugin-babel": "5.3.1",
		"eslint-plugin-css-modules": "2.11.0",
		"eslint-plugin-fp": "2.3.0",
		"eslint-plugin-import": "2.24.2",
		"eslint-plugin-jasmine": "4.1.2",
		"eslint-plugin-lodash-fp": "2.2.0-a1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.26.1",
		"html-webpack-plugin": "5.3.2",
		"mini-css-extract-plugin": "2.4.2",
		"mocha": "9.1.2",
		"node-sass": "6.0.1",
		"pnp-webpack-plugin": "1.7.0",
		"postcss": "8.3.9",
		"postcss-loader": "6.1.1",
		"prettier": "2.4.1",
		"prop-types": "15.7.2",
		"react-is": "17.0.2",
		"sass": "1.42.1",
		"sass-lint": "1.13.1",
		"sass-loader": "12.1.0",
		"style-loader": "3.3.0",
		"webpack": "5.58.1",
		"webpack-cli": "4.9.0",
		"webpack-dev-server": "4.3.1",
		"webpack-merge": "5.8.0"
	}
}
