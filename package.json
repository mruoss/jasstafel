{
	"name": "jasstafel",
	"version": "2.0.3",
	"description": "Jasstafel zum Schreiben von Jasspartien",
	"license": "MIT",
	"scripts": {
		"build": "webpack --env.mode production --env.presets serviceworker",
		"lint": "yarn run lint:js && yarn run lint:sass",
		"lint:js": "eslint src/js/",
		"lint:sass": "sass-lint -c .sass-lint.yml -v -q --max-warnings 0",
		"prettier": "prettier --write '**/*.js'",
		"start": "webpack-dev-server --content-base public/ --open --env.mode development --hot",
		"start:sw": "webpack-dev-server --content-base public/ --open --env.mode development --env.presets serviceworker",
		"test": "mocha --require babel-register ./tests/**/*.js"
	},
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^5.1.0",
		"bluebird": "^3.5.1",
		"core-js": "3.2.1",
		"konva": "^4.0.12",
		"lodash": "^4.17.19",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-konva": "^16.9.0-1",
		"react-redux": "^7.1.1",
		"react-sizes": "^2.0.0",
		"recompose": "^0.30.0",
		"redux": "^4.0.0",
		"redux-undo": "^1.0.0-beta9-9-7",
		"reselect": "^3.0.1",
		"styled-components": "^3.4.0",
		"workbox-webpack-plugin": "^5.1.4"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-0",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-env": "^7.6.0",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-module-resolver": "^3.1.1",
		"babel-polyfill": "^6.26.0",
		"chai": "^4.1.2",
		"chai-match-pattern": "^1.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.0.0",
		"eslint": "^7.12.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-import-resolver-babel-module": "^5.1.0",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-css-modules": "^2.7.5",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jasmine": "^2.10.1",
		"eslint-plugin-lodash-fp": "^2.2.0-a1",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.10.0",
		"file-loader": "^4.2.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^1.2.0",
		"mocha": "^6.2.0",
		"node-sass": "^4.9.0",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.13.6",
		"prop-types": "^15.6.2",
		"sass-lint": "^1.13.1",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"url-loader": "^2.1.0",
		"webpack": "^4.12.2",
